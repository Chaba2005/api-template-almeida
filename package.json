{
  "name": "api-commandee",
  "version": "1.0.0",
  "description": "API REST para o Commandee",
  "main": "dist/index.js",
  "scripts": {
    "start": "rollup -c && node dist/index.js",
    "build": "rollup -c",
    "dev": "nodemon",
    "format": "prettier --write \"**/*.{js,cjs,mjs,json,ts}\"",
    "lint": "eslint \"**/*.{js,cjs,mjs,ts}\" --fix"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@babel/eslint-parser": "^7.21.3",
    "@rollup/plugin-commonjs": "^24.0.1",
    "@rollup/plugin-json": "^6.0.0",
    "@rollup/plugin-node-resolve": "^15.0.2",
    "@rollup/plugin-terser": "^0.4.0",
    "@rollup/plugin-typescript": "^11.1.0",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "eslint": "^8.37.0",
    "nodemon": "^2.0.22",
    "prettier": "^2.8.7",
    "rollup": "^3.20.2",
    "ts-node": "^10.9.1",
    "tslib": "^2.5.0",
    "typescript": "^5.0.3"
  },
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "mysql2": "^3.2.0"
  },
  "nodemonConfig": {
    "ignore": [
      "node_modules",
      "dist"
    ],
    "watch": [
      "src"
    ],
    "ext": "ts",
    "exec": "ts-node src/index.ts"
  }
}
